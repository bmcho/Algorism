    "import sys\n",
    "\n",
    "# nCm = n! / (m! * (n-m)!) \n",
    "\n",
    "fac_dp = [1, 1, 2]\n",
    "n, m = map(int, sys.stdin.readline().split())\n",
    "\n",
    "for i in range(3, n + 1):\n",
    "\tfac_dp.append(fac_dp[i - 1] * i)\n",
    "\n",
    "print(fac_dp[n] // (fac_dp[m] * fac_dp[n - m]))\n"
   ]
  }
