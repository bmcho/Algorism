   "source": [
    "def solution(n):\n",
    "    ans = 0\n",
    "    \n",
    "    while n > 0:\n",
    "        if n % 2:\n",
    "            ans += 1\n",
    "            n = (n-1) // 2\n",
    "        else : n = n // 2\n",
    "    \n",
    "    return ans"
   ]
  }
