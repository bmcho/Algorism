    "def solution(n):\n",
    "    answer = 0\n",
    "    \n",
    "    if n <= 3:\n",
    "        return n\n",
    "    \n",
    "    dp = [0] * (n+1)\n",
    "    dp[1],dp[2],dp[3] = 1,2,3\n",
    "\n",
    "    for i in range(4,n+1):\n",
    "        dp[i] = (dp[i-2] + dp[i-1]) % 1234567\n",
    "\n",
    "    return dp[-1]"
